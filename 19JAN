#include<stdio.h>
int main(){
    int arr[10];
    printf("Enter value at index 7: ");
    scanf("%d", &arr[7]);

    printf("Enter value at index 9: ");
    scanf("%d", &arr[9]);

    int n=sizeof(arr)/sizeof(arr[0]);
    for(int i=0 ; i<n ; i++){
        printf("Array[%d] = %d\n",i,arr[i]);
    }
    
    // OUTPUT:
    // Array[0] = 1972759120
    // Array[1] = -539058237
    // Array[2] = -2
    // Array[3] = 6422280
    // Array[4] = 1972735005
    // Array[5] = 4200880
    // Array[6] = 6422356
    // Array[7] = 56
    // Array[8] = 4200880
    // Array[9] = 35
}
